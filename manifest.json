{
  "name": "AlgoSolve",
  "description": "알고 솔브의 확장 프로그램 입니다.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.acmicpc.net/status?*from_mine=1*"
      ],
      "js": [
        "./platform_listener/result_state.js",
        "./platform_listener/platform.js",
        "./platform_listener/sender.js",
        "./platform_listener/boj/explanation_status.js",
        "./platform_listener/boj/process.js",
        "./platform_listener/boj/index.js"
      ]
    },
    {
      "matches": [
        "https://school.programmers.co.kr/learn/courses/*/lessons/*"
      ],
      "js": [
        "./platform_listener/result_state.js",
        "./platform_listener/platform.js",
        "./platform_listener/sender.js",
        "./platform_listener/programers/process.js",
        "./platform_listener/programers/index.js"
      ]
    }
  ]
}